{
    "name": "wjdhollow/antglob",
    "description": "PHP Ant Glob",
    "keywords": ["wjdhollow", "php", "glob"],
    "homepage": "https://github.com/wjdhollow/antglob",
    "license": "MIT",
    "authors": [
        {
            "name": "William Holloway",
            "email": "wjdhollow@gmail.com"
        }
    ],
    "bin": [
      "antglob"
    ],
    "config": {
      "bin-dir": "bin"
    },
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "Wjdhollow\\Glob\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wjdhollow\\Glob\\Test\\": "test/"
        }
    }
}
